<h3>Mis Reservaciones</h3>
<table class="table">
	<thead>
		<tr>
			<th width="10%">Nombre</th>
			<th width="50%">Foto</th>
			<th width="10%">Fecha</th>
			<th colspan="3"></th>
		</tr>
	</thead>
	<tbody>
		<% @reservations.each do |reservation| %>
			<tr>
				<td><%= link_to reservation.residence.name, reservation.residence %></td>
				<td><img src="<%= reservation.residence.photo%>" width="300"></td>
				<td><%= reservation.in_date.strftime("%d-%m-%Y") %></td>
			</tr>
		<% end %>
	</tbody>
</table>