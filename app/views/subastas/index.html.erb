<h3>Subastas</h3>
<table class="table">
	<thead>
		<tr>
			<th width="20%">Nombre</th>
			<th width="20%">Monto</th>
			<th width="60%">Foto</th>
			<th colspan="3"></th>
		</tr>
	</thead>
	<tbody>
		<% @subastas.each do |subasta| %>
			<tr>
				<td><%= subasta.nombre %></td>
				<td><%= subasta.monto %></td>
				<td>sinFoto</td>
				<td><%= link_to 'Editar', subastas_path, class: 'btn btn-warning' %></td>
				<td><%= link_to 'Eliminar', subasta_path, method: :delete, class: 'btn btn-danger', data: { confirm: 'Â¿realmente desea eliminar la subasta?' } %></td>
			</tr>
		<% end %>
	</tbody>
</table>
